<odoo>
    <data>
        <record model="ir.ui.view" id="gestionit_pe_fe.view_stock_warehouse_form">
            <field name="name">Series permitidad</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='code']" position="after">
                    <field name="journal_ids" widget="many2many_tags" options="{'no_create': True, 'no_open': True}" domain="[('invoice_type_code_id','=',['01','03','07','08'])]"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>